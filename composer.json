{
    "name": "canciolabs/functions",
    "description": "Functions that do not deserve a class or component. Not yet.",
    "type": "library",
    "keywords": ["functions", "php", "utilities", "helpers"],
    "homepage": "https://bitbucket.org/cancio-labs/functions/src/master/",
    "license": "MIT",
    "authors": [
        {
            "name": "Nilo Soares",
            "email": "nilosoares@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "webmozart/assert": "1.10.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "files": [
            "src/array_functions.php",
            "src/utility_functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CancioLabs\\Functions\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "tests": [
            "vendor/bin/phpunit"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage-html=coverage"
        ]
    }
}
